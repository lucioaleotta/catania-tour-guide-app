{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "@components/*": [
        "./app/components/*"
      ],
      "@features/*": [
        "./app/features/*"
      ],
      "@assets/*": [
        "./app/assets/*"
      ],
      "@services/*": [
        "./app/services/*"
      ],
      "@hooks/*": [
        "./app/hooks/*"
      ]
    },
    "jsx": "preserve",
    "typeRoots": ["./node_modules/@types", "./types"]
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
